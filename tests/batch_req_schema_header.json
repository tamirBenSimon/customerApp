{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "ids": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          }
        }
      }
    },
    "BatchRequest": {
      "type": "object",
      "properties": {
        "BatchHeader": {
          "type": "object",
          "properties": {
            "Username": {
              "type": "string"
            },
            "Password": {
              "type": "string"
            },
            "CustomerBatchId": {
              "type": "string"
            },
            "CreationTimeStamp": {
              "type": "string"
            },
            "OrdersQuantity": {
              "type": "string"
            },
            "ShipTime": {
              "type": "string"
            },
            "ShipDate": {
              "type": "string"
            },
            "Currency": {
              "type": "string"
            },
            "Payments": {
              "type": "object",
              "properties": {
                "FreightPaymentType": {
                  "type": "string"
                },
                "DutiesPaymentType": {
                  "type": "string"
                }
              }
            },
            "Labels": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "LabelFormat": {
                    "type": "string"
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
